{
  "source": {
    "include": [
      "./"
    ],
    "includePattern": "\\.(js|jsx)$",
    "exclude": [
      "node_modules/"
    ]
  },
  "opts": {
    "destination": "./docs/"
  },
  "plugins": [
    "./jsdoc-rdf.js"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "rdfConfig": {
    "baseUri": "https://your-project.com/api/"
  }
}