{
  "source": {
    "include": ["./example.js"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "*.test.js"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["./jsdoc-rdf.js"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "rdfConfig": {
    "baseUri": "https://example.com/api/"
  },
  "markdown": {
    "idInHeadings": true
  }
}